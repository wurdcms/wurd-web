{
  "name": "wurd-web",
  "version": "8.0.0-beta.6",
  "description": "Vanilla JS browser client for the Wurd CMS",
  "main": "dist/wurd.cjs.js",
  "module": "dist/wurd.esm.js",
  "browser": "dist/wurd.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "mocha --require @babel/register --colors ./src/*.spec.js",
    "test-file": "mocha --require @babel/register --colors -w",
    "test:watch": "mocha --require @babel/register --colors -w ./src/*.spec.js",
    "example": "serve examples/simple"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/register": "^7.18.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-eslint": "^8.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "marked": "^4.0.18",
    "mocha": "^10.0.0",
    "rollup": "^2.78.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^14.0.1",
    "sinon": "^14.0.0"
  },
  "keywords": [
    "cms",
    "headless",
    "content",
    "browser"
  ],
  "author": "Charles Davison",
  "license": "MIT",
  "dependencies": {
  },
  "files": [
    "*.md",
    "dist",
    "src",
    "examples"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wurdcms/wurd-web.git"
  },
  "homepage": "https://github.com/wurdcms/wurd-web"
}
