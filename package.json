{
  "name": "wurd-web",
  "version": "7.2.0",
  "description": "Vanilla JS browser client for the Wurd CMS",
  "main": "dist/wurd.cjs.js",
  "module": "dist/wurd.esm.js",
  "browser": "dist/wurd.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "mocha --require @babel/register --colors ./src/*.spec.js",
    "test:watch": "mocha --require @babel/register --colors -w ./src/*.spec.js",
    "example": "serve examples/simple"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/register": "^7.8.6",
    "mocha": "^7.1.0",
    "node-fetch": "^2.6.0",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.4",
    "serve": "^11.3.0",
    "sinon": "^9.0.1"
  },
  "keywords": [
    "cms",
    "headless",
    "content",
    "browser"
  ],
  "author": "Charles Davison",
  "license": "MIT",
  "dependencies": {
    "get-property-value": "^2.0.0",
    "marked": "^0.8.0"
  },
  "files": [
    "*.md",
    "dist",
    "src",
    "examples"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wurdcms/wurd-web.git"
  },
  "homepage": "https://github.com/wurdcms/wurd-web"
}
